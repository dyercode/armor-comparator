{
  "name": "armorcomparator",
  "description": "An app for comparing various armor option for pathfinder d20",
  "version": "0.0.1",
  "private": true,
  "author": "Jon Dyer",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/register": "^7.11.5",
    "bs-platform": "^8.2.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "dev": "bsb -make-world && parcel app/index.html",
    "build-old": "bsb-make world && parcel build src/index.html --public-url .",
    "lint": "eslint app --ext .js,.jsx,.ts,.tsx",
    "test": "bsb -make-world && mocha --require @babel/register"
  },
  "dependencies": {
    "knockout": "^3.5.1",
    "knockout-mapping": "^2.6.0"
  }
}
