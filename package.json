{
  "name": "armorcomparator",
  "description": "An app for comparing various armor option for pathfinder d20",
  "version": "0.0.1",
  "private": true,
  "author": "Jon Dyer",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/register": "^7.11.5",
    "chai": "^4.2.0",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.3",
    "eslint": "^7.8.1",
    "mocha": "^8.1.3",
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "dev": "parcel app/index.html",
    "build": "parcel build src/index.html --public-url .",
    "build-elm": "elm make src/Main.elm --output=main.js",
    "lint": "eslint app --ext .js,.jsx,.ts,.tsx",
    "test": "mocha --require @babel/register"
  },
  "dependencies": {
    "knockout": "^3.5.1",
    "knockout-mapping": "^2.6.0"
  }
